<!DOCTYPE suppressions PUBLIC
  "-//Checkstyle//DTD SuppressionFilter Configuration 1.2//EN"
  "https://checkstyle.org/dtds/suppressions_1_2.dtd">

<suppressions>

  <!-- TODO: Complete Javadocs -->
  <suppress checks="(MissingJavadocMethod|MissingJavadocType|SummaryJavadoc)" />

  <!-- Allowing "I" prefix to denote interface -->
  <suppress
    checks="AbbreviationAsWordInName"
    files="[/\\]com[/\\]aoapps[/\\]servlet[/\\]subrequest[/\\]IHttpServletSubRequest\.java$"
    message="'IHttpServletSubRequest'"
  />
  <suppress
    checks="AbbreviationAsWordInName"
    files="[/\\]com[/\\]aoapps[/\\]servlet[/\\]subrequest[/\\]IHttpServletSubResponse\.java$"
    message="'IHttpServletSubResponse'"
  />
  <suppress
    checks="AbbreviationAsWordInName"
    files="[/\\]com[/\\]aoapps[/\\]servlet[/\\]subrequest[/\\]IServletSubRequest\.java$"
    message="'IServletSubRequest'"
  />
  <suppress
    checks="AbbreviationAsWordInName"
    files="[/\\]com[/\\]aoapps[/\\]servlet[/\\]subrequest[/\\]IServletSubResponse\.java$"
    message="'IServletSubResponse'"
  />

  <!-- Will continue using "RFC" prefix -->
  <suppress
    checks="AbbreviationAsWordInName"
    files="[/\\]com[/\\]aoapps[/\\]servlet[/\\]subrequest[/\\]HttpServletSubResponse\.java$"
    message="'formatRFC5322'"
  />

  <!-- Consistency with standard "URLDecoder", "URLEncoder", and "URL" -->
  <suppress
    checks="AbbreviationAsWordInName"
    files="[/\\]com[/\\]aoapps[/\\]servlet[/\\]subrequest[/\\]UnmodifiableCopyHttpServletRequest\.java$"
    message="'(requestURI|requestedSessionIdFromURL)'"
  />

</suppressions>
